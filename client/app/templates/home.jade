#no-app-message.center
    p!= t('no-app-message')

#app-list

.app-tools
  .machine-infos
    .memory
      div
        = t('Memory consumption')
        | &nbsp;(#{t('total')}: <span class="total"></span>)
      .progress
        .bar

    .disk
      div
        = t('Disk consumption')
        | &nbsp;(#{t('total')} : <span class="total"></span>)
      .progress
        .bar


  .btn-group
      button#manage-app-button.btn= t('manage')
